#!/bin/bash

echo ""
echo "Applying migration BusinessDetailsNotFound"

echo "Adding routes to conf/app.routes"
echo "" >> ../conf/app.routes
echo "GET        /businessDetailsNotFound                       controllers.BusinessDetailsNotFoundController.onPageLoad()" >> ../conf/app.routes

echo "Adding messages to conf.messages"
echo "" >> ../conf/messages.en
echo "businessDetailsNotFound.title = businessDetailsNotFound" >> ../conf/messages.en
echo "businessDetailsNotFound.heading = businessDetailsNotFound" >> ../conf/messages.en

echo "Migration BusinessDetailsNotFound completed"
